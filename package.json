{
  "name": "@cronocode/react-box",
  "version": "3.1.1",
  "type": "module",
  "main": "./box.cjs",
  "module": "./box.mjs",
  "types": "./box.d.ts",
  "exports": {
    ".": {
      "types": "./box.d.ts",
      "import": "./box.mjs",
      "require": "./box.cjs"
    },
    "./ssg": {
      "types": "./ssg.d.ts",
      "import": "./ssg.mjs",
      "require": "./ssg.cjs"
    },
    "./components/*": {
      "types": "./components/*.d.ts",
      "import": "./components/*.mjs",
      "require": "./components/*.cjs"
    },
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "scripts": {
    "dev": "vite ./pages --config ./pages.vite.config.ts --mode dev",
    "build:pages": "vite build ./pages --config ./pages.vite.config.ts --outDir ../dist --mode production",
    "preview": "vite preview",
    "build": "vite build",
    "postbuild": "cp package.json dist & cp LICENSE dist & cp README.md dist",
    "build:dev": "vite build --mode dev",
    "compile": "tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "test:all": "vitest run",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cronocodesolutions/react-box.git"
  },
  "keywords": [
    "react",
    "box"
  ],
  "author": "Maxim Procopenco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cronocodesolutions/react-box/issues"
  },
  "homepage": "https://github.com/cronocodesolutions/react-box#readme",
  "description": "",
  "dependencies": {
    "@cronocode/identity-factory": "^0.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@rollup/pluginutils": "^5.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.5.5",
    "@types/postcss-mixins": "^9.0.5",
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^15.7.4",
    "prettier": "^3.3.3",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.3",
    "vite-plugin-dts": "^4.2.1",
    "vitest": "^3.2.4"
  }
}
