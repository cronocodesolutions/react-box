{
  "name": "@cronocode/react-box",
  "version": "1.2.4",
  "main": "./box.mjs",
  "module": "./box.mjs",
  "types": "./box.d.ts",
  "exports": {
    ".": {
      "import": "./box.mjs",
      "require": "./box.mjs",
      "types": "./box.d.ts"
    },
    "./style.css": "./style.css",
    "./theme": {
      "import": "./theme.mjs",
      "require": "./theme.mjs",
      "types": "./theme.d.ts"
    },
    "./components/*": {
      "import": "./components/*.mjs",
      "require": "./components/*.mjs",
      "types": "./components/*.d.ts"
    }
  },
  "scripts": {
    "dev": "vite ./dev --config ./vite.config.ts --mode dev",
    "build": "vite build",
    "build:dev": "vite build --mode dev",
    "postbuild": "cp package.json dist & cp LICENSE dist & rm ./dist/index.d.ts & rm ./dist/index.js",
    "compile": "tsc --noEmit --skipLibCheck",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cronocodesolutions/react-box.git"
  },
  "keywords": [
    "react",
    "box"
  ],
  "author": "Maxim Procopenco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cronocodesolutions/react-box/issues"
  },
  "homepage": "https://github.com/cronocodesolutions/react-box#readme",
  "description": "",
  "devDependencies": {
    "@cronocode/identity-factory": "^0.0.6",
    "@rollup/pluginutils": "^5.0.4",
    "@types/node": "^20.6.0",
    "@types/postcss-mixins": "^9.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "postcss-each": "^1.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3"
  }
}
