{
  "name": "@cronocode/react-box",
  "version": "1.4.6",
  "main": "./box.cjs",
  "module": "./box.mjs",
  "types": "./box.d.ts",
  "exports": {
    ".": {
      "import": "./box.mjs",
      "require": "./box.cjs",
      "types": "./box.d.ts"
    },
    "./style.css": "./style.css",
    "./theme": {
      "import": "./theme.mjs",
      "require": "./theme.cjs",
      "types": "./theme.d.ts"
    },
    "./components/*": {
      "import": "./components/*.mjs",
      "require": "./components/*.cjs",
      "types": "./components/*.d.ts"
    },
    "./plugins": {
      "import": "./plugins.mjs",
      "require": "./plugins.cjs",
      "types": "./plugins.d.ts"
    }
  },
  "scripts": {
    "dev": "vite ./pages --config ./pages.vite.config.ts --mode dev",
    "build:pages": "vite build ./pages --config ./pages.vite.config.ts --outDir ../dist --mode production",
    "preview": "vite preview",
    "build": "vite build",
    "build:dev": "vite build --mode dev",
    "postbuild": "cp package.json dist & cp LICENSE dist & rm ./dist/index.d.ts & rm ./dist/index.js",
    "compile": "tsc --noEmit --skipLibCheck",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cronocodesolutions/react-box.git"
  },
  "keywords": [
    "react",
    "box"
  ],
  "author": "Maxim Procopenco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cronocodesolutions/react-box/issues"
  },
  "homepage": "https://github.com/cronocodesolutions/react-box#readme",
  "description": "",
  "devDependencies": {
    "@cronocode/identity-factory": "^0.0.6",
    "@rollup/pluginutils": "^5.0.2",
    "@types/node": "^18.15.7",
    "@types/postcss-mixins": "^9.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "postcss-each": "^1.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.7",
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0"
  }
}
