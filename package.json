{
  "name": "@cronocode/react-box",
  "version": "0.4.5",
  "main": "./box.js",
  "module": "./box.js",
  "types": "./box.d.ts",
  "exports": {
    ".": {
      "import": "./box.js",
      "require": "./box.js",
      "types": "./box.d.ts"
    },
    "./style.css": "./style.css",
    "./components/*": {
      "import": "./components/*.js",
      "require": "./components/*.js",
      "types": "./components/*.d.ts"
    }
  },
  "scripts": {
    "dev": "vite ./dev --config ./vite.config.ts --mode dev",
    "build": "vite build",
    "build:dev": "vite build --mode dev",
    "postbuild": "cp package.json dist & cp LICENSE dist & rm ./dist/index.d.ts & rm ./dist/index.js",
    "compile": "tsc --noEmit --skipLibCheck",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cronocodesolutions/react-box.git"
  },
  "keywords": [
    "react",
    "box"
  ],
  "author": "Maxim Procopenco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cronocodesolutions/react-box/issues"
  },
  "homepage": "https://github.com/cronocodesolutions/react-box#readme",
  "description": "",
  "devDependencies": {
    "@cronocode/identity-factory": "^0.0.6",
    "@rollup/pluginutils": "^5.0.2",
    "@types/node": "^18.7.14",
    "@types/postcss-mixins": "^9.0.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "postcss": "^8.4.16",
    "postcss-mixins": "^9.0.4",
    "prettier": "^2.7.1",
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.1"
  }
}
